function filter1_1()

[taoP1,taoP2] = meshgrid(0:0.01:1,0:0.01:1);
grid = [taoP1(:) taoP2(:)];

a = grid(:, 1)+grid(:, 2);
b = grid(:, 1).*grid(:, 2);

M = [];
for i=1:length(a)
    fun = @(x)-4*(-(1 + a(i)^2 -2*b(i))*x^2 + x*(1-b(i)));
    M(i) = fun((1-b(i))/(2*(a(i)^2-2*b(i)+1)));
end
fun = @(x)-4.*(-(1 + a.^2 -2.*b).*x.^2 + x.*(1-b));
M = 

plot3(grid(:, 1), grid(:, 2), M, '.');
axis equal
xlabel('taoP1')
ylabel('taoP2')
zlabel('f')
end